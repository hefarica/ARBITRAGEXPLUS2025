# Copy this file to `.env` and fill in your secrets.
PORT=3000
CHAIN_IDS=1,137,56
ETH_RPC_URL=
POLYGON_RPC_URL=
BSC_RPC_URL=
WALLET_PRIVATE_KEY=
ENGINE_RPC_URL=http://localhost:4000

# Google Sheets Integration
# SPREADSHEET_ID: The ID of your Google Spreadsheet.
# GOOGLE_APPLICATION_CREDENTIALS: Path to your Google Service Account JSON key file.
# For local development, place your service account JSON key in the 'keys' directory
# and set this variable to './keys/gsheets-sa.json'.
# For deployment, consider using base64 encoded JSON content as an environment variable.
SPREADSHEET_ID=
GOOGLE_APPLICATION_CREDENTIALS=./keys/gsheets-sa.json

# Google Cloud Project ID (optional, for some Google Cloud operations)
GOOGLE_PROJECT_ID=

# Google Service Account credentials (if using direct JSON content in .env, not recommended for production)
# GOOGLE_CLIENT_EMAIL=
# GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"

# Specific Sheets IDs (if applicable, otherwise use SPREADSHEET_ID)
SHEETS_PNL_SPREADSHEET_ID=
SHEETS_LIMITS_SPREADSHEET_ID=

